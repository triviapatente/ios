# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Trivia Patente' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  # Pods for Trivia Patente
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'SwiftyJSON', '~> 4.0'
  pod 'Alamofire', '~> 4.0'
  pod 'Socket.IO-Client-Swift', '~> 12.0.0' # Or latest version
  pod 'MBProgressHUD', '~> 1.0.0'
  pod 'SideMenu'
  pod 'SDWebImage', '~> 4.0'
  pod 'Google-Mobile-Ads-SDK'
  pod 'Fabric', '~> 1.7.2'
  pod 'Crashlytics', '~> 3.9.3'
  # pod 'RSKImageCropper', '2.0.0'
  pod 'CropViewController', '2.6.1'
  pod "CollieGallery", '0.6.0' #no updates
  pod 'BulletinBoard', '1.3.0'

  target 'Trivia PatenteTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Trivia PatenteUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

post_install do |installer|
  installer.pods_project.targets.each do |target|
    target.build_configurations.each do |config|
      config.build_settings["IPHONEOS_DEPLOYMENT_TARGET"] = "11.0";
    end
  end
end
